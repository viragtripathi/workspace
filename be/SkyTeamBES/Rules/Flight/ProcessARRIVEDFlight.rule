rule Rules.Flight.ProcessARRIVEDFlight {
	attribute {
		priority = 5;
		forwardChain = true;
	}
	declare {
		
		Concepts.Flight flight;
	}
	when {
		flight.EventType == Scorecards.FlightEventTypes.ARRIVED;
	}
	then {
		System.debugOut("Flight ARRIVED event received");
	}
}