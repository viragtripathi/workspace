rule Rules.Flight.ProcessBOARDINGFlight {
	attribute {
		priority = 5;
		forwardChain = true;
	}
	declare {
		
		Concepts.Flight flight;
	}
	when {
		flight.EventType == Scorecards.FlightEventTypes.BOARDING;
	}
	then {
		System.debugOut("Flight BOARDING event received");
	}
}